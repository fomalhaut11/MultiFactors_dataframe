# 数据库配置环境变量
# 复制此文件为 .env 并填入实际值

# 主数据库配置
DB_HOST=your_database_host
DB_USER=your_username
DB_PASSWORD=your_password
DB_DATABASE=stock_data

# 其他数据库配置
DB_MIN_DATABASE=stock_min1
DB_JQ_DATABASE=jqdata
DB_WIND_DATABASE=Wind

# 其他配置（可选）
DATA_ROOT=E:\Documents\PythonProject\StockProject\StockData
LOG_LEVEL=INFO
PARALLEL_JOBS=-1

# 编码配置 (解决Windows GBK编码问题)
PYTHONIOENCODING=utf-8
LOG_ENCODING=utf-8
# 禁用emoji输出 (项目规范要求)
USE_EMOJI=false
FORCE_ASCII_OUTPUT=true